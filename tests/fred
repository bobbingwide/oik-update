<!-- wp:heading -->
<h2 id="notes">Notes</h2>
<!-- /wp:heading -->

<!-- wp:list -->
<ul><li>This plugin may one day be merged into the Easy Digital Downloads plugin. It hadn't been done by June 2020.</li><li>It produces Warnings in PHP 8.0</li></ul>
<!-- /wp:list -->

<!-- wp:preformatted -->
